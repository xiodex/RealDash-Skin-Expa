				<action name="xa-fade-zone-navi-MAP-out"></action>
						
				<!-- Cam mode -->
				<action name="xa-fade-folder-CAM-out"></action>
				
            </actions>
        </trigger>
		
		<trigger name="xt-POWER-mode" condition="equal" variable="Dummy variable 8" tolerance="2.00" reset="0.00" cooldown="0.00">
            <actions>
				
				<!-- Car mode -->
                <action name="xa-fade-folder-CAR-out"></action>
				<action name="xa-fade-zone-car-BODY-out"></action>
				<action name="xa-fade-zone-car-ALL-out"></action>
				<action name="xa-morph-zone-car-BODY-out"></action>			
				
